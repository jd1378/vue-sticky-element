{
  "name": "vue-sticky-element",
  "version": "1.7.0",
  "description": "A simple vue sticky component wrapper that will stick to screen when scrolled past it",
  "author": "jd1378",
  "license": "MIT",
  "main": "dist/vue-sticky-element.ssr.js",
  "browser": "dist/vue-sticky-element.esm.js",
  "module": "dist/vue-sticky-element.esm.js",
  "unpkg": "dist/vue-sticky-element.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "!src/serve-dev.*"
  ],
  "keywords": [
    "vue",
    "sticky",
    "element",
    "sticky-element",
    "sticky-component",
    "sfc",
    "component",
    "header",
    "navbar"
  ],
  "scripts": {
    "serve": "vue-cli-service serve src/serve-dev.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "prepublishOnly": "npm run build",
    "audit-fix": "npx cross-env npm_config_yes=true npx yarn-audit-fix"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jd1378/vue-sticky-element.git"
  },
  "dependencies": {
    "v-scroll-threshold": "^2.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/runtime-dom": "^3.2.37",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-vue": "^7.4.0",
    "minimist": "^1.2.0",
    "prettier": "^2.2.1",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "^2.7.14",
    "vue-template-compiler": "^2.7.7"
  },
  "peerDependencies": {
    "vue": "^2.6.10 || >=3.0.0"
  },
  "engines": {
    "node": ">=8"
  }
}
